<%= simple_form_for dog, data: { insert_in_list_target: "form", action: "submit->insert-in-list#send" } do |f| %>
  <%= f.input :name %>
  <%= f.input :breed %>
  <%= f.input :age %>
  <%= f.input :price %>
  <%= f.input :size, collection: %w[Extra-Small Small Medium Large Extra-Large] %>
  <%= f.input :description %>
  <%= f.input :photo, as: :file %>
  <%= f.button :submit %>
<% end %>
